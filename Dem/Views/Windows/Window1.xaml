<Window x:Class="Dem.Views.Windows.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dem.Views.Windows"
        mc:Ignorable="d"
        Title="Добовление продукции на склад" Height="219" Width="377">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Введите количество продукцти" Margin="10"/>
            <TextBox HorizontalAlignment="Left" Width="100" x:Name="QuantityTextBox" Margin="10"/>
            <Button Style="{StaticResource FormButton}" HorizontalAlignment="Left" Width="50" Content="OK" IsDefault="True" Click="OnOkClick" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
